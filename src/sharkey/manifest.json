{
    "control": {
        "Package": "sharkey",
        "Version": "0.1.0",
        "Architecture": "all",
        "Maintainer": "alyxshang",
        "Depends": "rust build-essential binutils openssl",
        "Homepage": "https://github.com/alyxshang/sharkey.rs",
        "Description": ["A tiny library and CLI tool to perform actions on Sharkey using Rust."]
    },
    "installation_prefix": "/data/data/com.termux/files/usr",
